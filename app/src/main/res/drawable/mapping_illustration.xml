<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="400dp"
    android:height="400dp"
    android:viewportWidth="400"
    android:viewportHeight="400">

    <!-- Background -->
    <path
        android:fillColor="#F5F7FA"
        android:pathData="M200,200 m-150,0 a150,150 0 1,0 300,0 a150,150 0 1,0 -300,0" />

    <!-- Floor plan outline -->
    <path
        android:strokeColor="#002B5C"
        android:strokeWidth="4"
        android:fillColor="#00000000"
        android:pathData="M100,100 h200 v200 h-200 Z" />

    <!-- Rooms -->
    <path
        android:strokeColor="#002B5C"
        android:strokeWidth="2"
        android:fillColor="#00000000"
        android:pathData="M120,120 h60 v60 h-60 Z
                         M220,120 h60 v60 h-60 Z
                         M120,220 h60 v60 h-60 Z
                         M220,220 h60 v60 h-60 Z" />

    <!-- Room labels -->
    <path
        android:fillColor="#CD9C3B"
        android:fillAlpha="0.3"
        android:pathData="M130,140 h40 v20 h-40 Z
                         M230,140 h40 v20 h-40 Z
                         M130,240 h40 v20 h-40 Z
                         M230,240 h40 v20 h-40 Z" />

    <!-- Current location indicator -->
    <path
        android:fillColor="#F44336"
        android:pathData="M190,200 m-10,0 a10,10 0 1,0 20,0 a10,10 0 1,0 -20,0" />
</vector>